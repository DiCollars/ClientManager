@model ClientManagerMVC.JsonModels.UserJson
@{
    ViewData["Title"] = "Update user";
}
<form>
    <div class="form-group">
        <input type="text" class="form-control" placeholder="Enter name" name="Name" value="@Model.Name">
    </div>
    <div class="form-group">
        <input type="text" class="form-control" placeholder="Enter last name" name="LastName" value="@Model.LastName">
    </div>
    <div class="form-group">
        <input type="email" class="form-control" placeholder="Enter email" name="Email" value="@Model.Email">
    </div>
    <button type="submit" class="btn btn-dark">Submit</button>
</form>